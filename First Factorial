/* Problem: Have the function FirstFactorial(num) take the num parameter being passed and return the factorial of it 
 * (ie. if num = 4, return (4 * 3 * 2 * 1)). For the test cases, the range will be between 1 and 18. 
*/

import java.util.*; 
import java.io.*;

class Function {  
  int FirstFactorial(int num) { 
	// FirstFactorial accepts an integer and computes the factorial of that integer. Returns an integer. 
    int result = 1;
    for (int i = num; i >= 2; i--){
    	result *= i; 
    }
    return result;
  } 
  
  public static void main (String[] args) {     
    Scanner  s = new Scanner(System.in);
    Function c = new Function();
    System.out.print(c.FirstFactorial(s.nextLine())); 
  }   
  
}
