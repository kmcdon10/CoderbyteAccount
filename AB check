import java.util.*; 
import java.io.*;

class Function {  
  String ABCheck(String str) {
  // ABCCheck takes a string str and returns a string "true" if the characters a and b are 
  // separated by exactly 3 places anywhere in the stirng at least once. Otherwise, returns "false".
    
   for (int i = 0; i < str.length()-4; i++){
      if ((str.charAt(i) == 'a' || str.charAt(i) == 'A') && (str.charAt(i+4) == 'b' || str.charAt(i+4) == 'B')){
        return "true";
      }
    }
    return "false";
  } 
  
  public static void main (String[] args) {     
    Scanner  s = new Scanner(System.in);
    Function c = new Function();
    System.out.print(c.ABCheck(s.nextLine())); 
  }   
  
}
